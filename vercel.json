{
  "buildCommand": "npx prisma db push --force-reset && npm run db:seed && npm run build",
  "installCommand": "npm install --force && npx prisma generate",
  "framework": "nextjs",
  "build": {
    "env": {
      "NODE_OPTIONS": "--max-old-space-size=4096"
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
