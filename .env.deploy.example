# Compose deployment env
# Fill these and run: docker compose --env-file .env.deploy up -d --build

# Domain & email for automatic HTTPS via Caddy
DOMAIN=your-domain.com
EMAIL=admin@your-domain.com

# Postgres config (used by db service and DATABASE_URL)
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=jelantahgo

# App secrets
JWT_SECRET=change-me-very-strong
